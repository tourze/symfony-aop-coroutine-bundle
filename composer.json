{
  "name": "tourze/symfony-aop-coroutine-bundle",
  "description": "协程支持",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-session": "*",
    "doctrine/orm": "^2.20 || ^3.0",
    "monolog/monolog": "^3.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/http-kernel": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/service-contracts": "^3.5",
    "tourze/symfony-aop-bundle": "~0.0.3",
    "tourze/symfony-runtime-context-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopCoroutineBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopCoroutineBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}